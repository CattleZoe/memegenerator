{
  "name": "meme-generator",
  "version": "1.0.0",
  "description": "Easily add text to images or memes.",
  "source": "src/index.html",
  "scripts": {
    "lint": "eslint . --ext .js",
    "clean": "rimraf docs .parcel-cache",
    "generateSW": "workbox generateSW",
    "start": "parcel",
    "build:parcel": "parcel build --dist-dir=docs --public-url=./",
    "build": "npm-run-all clean build:parcel",
    "postbuild": "npm run generateSW"
  },
  "keywords": [
    "meme",
    "meme-generator",
    "meme-maker"
  ],
  "author": "George Raptis <georapbox@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-raw-url": "~2.10.3",
    "@parcel/transformer-webmanifest": "~2.10.3",
    "eslint": "~8.56.0",
    "npm-run-all": "~4.1.5",
    "parcel": "~2.10.3",
    "rimraf": "~5.0.5",
    "workbox-cli": "~7.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "@georapbox/capture-photo-element": "~3.2.1",
    "@georapbox/files-dropzone-element": "~1.2.1",
    "@georapbox/modal-element": "~1.2.1",
    "@georapbox/web-share-element": "~3.1.0",
    "bootstrap": "~4.6.2"
  }
}
